// Place your key bindings in this file to overwrite the defaults
[
	{
		"key": "ctrl+i",
		"command": "editor.action.formatDocument"
    },
    {
        // don't close window if no tabs are open
        "key": "cmd+w",
        "command": "-workbench.action.closeWindow",
        "when": "!editorIsOpen"
	}
]
